{
    "name": "technetist/taskr",
    "description": "A CLI tool for tracking time on tasks using Toggl and Jira",
    "keywords": [
        "time tracking",
        "worklog",
        "productivity",
        "console",
        "cli"
    ],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/technetist/taskr/issues",
        "source": "https://github.com/technetist/taskr"
    },
    "authors": [
        {
            "name": "Adrien Maranville",
            "email": "adrien@technetist.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.2",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-global-assertions": "^1.0",
        "spatie/laravel-ray": "^1.31"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "sniff": [
            "./vendor/bin/pint --test"
        ],
        "lint": [
            "./vendor/bin/pint"
        ],
        "test": "./vendor/bin/pest"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "taskr"
    ],
    "version": "0.0.1"
}
